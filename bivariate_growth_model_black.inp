title:
 bivariate growth model autonomic space
 
data:
FILE = all.long0.csv;

variable:
names are
ID	study	task	RSA	PEP
RSAz	PEPz	RSAb	PEPb	gender	feedback
ps2	psc	rt2	rt3
baseline	math	prep	qa	recover speech
female	negative	positive	RSAbc	PEPbc
black age bmi;

USEVARIABLES = rsaz pepz rsabc female black;
!math prep qa recover speech; 
!rt2 psc positive;
within = rsaz pepz;
!math prep qa recover speech;
between = female rsabc black;
!rt2 psc positive;

CLUSTER = id;

missing = .;
	
ANALYSIS:
	coverage = 0;
	!model = nocovariances;
	TYPE = TWOLEVEL RANDOM;
	ESTIMATOR = ML;

MODEL:

%WITHIN%

S|rsaz on pepz;

[rsaz@0 pepz@0];

%BETWEEN%
 
S; [S];

s on female black; 

s on rsabc (b);

rsabc on female (a1);

rsabc on black (a2);

MODEL CONSTRAINT:

NEW(ind1);

ind=a1*b;

NEW(ind2);

ind=a2*b;


OUTPUT: SAMPSTAT STANDARDIZED CINTERVAL;