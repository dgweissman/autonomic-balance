title:
 bivariate growth model autonomic space
 
data:
FILE = all.long0.csv;

variable:
names are
ID	study	task	RSA	PEP
RSAz	PEPz	RSAb	PEPb	gender	feedback
ps2	psc	rt2	rt3
baseline	math	prep	qa	recover speech
female	negative	positive	RSAbc	PEPbc
black age bmi; 

USEVARIABLES =
rsabc pepbc rsaz pepz black;


within = rsaz pepz;

between = 
rsabc pepbc black;


CLUSTER = id;

missing = .;
	
ANALYSIS:
	coverage = 0;
	!model = nocovariances;
	TYPE = TWOLEVEL RANDOM;
	ESTIMATOR = ML;

MODEL:

%WITHIN%

S|pepz on rsaz;

[rsaz@0 pepz@0];

%BETWEEN%
 
S; [S]; 

s on black;

!s on rsabc (b);

s on pepbc;

pepbc on black;

rsabc on black (a1);

!MODEL CONSTRAINT:

!NEW(ind1);
!ind1=a1*b;


OUTPUT: SAMPSTAT STANDARDIZED CINTERVAL;